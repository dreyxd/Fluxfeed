{
  "redirects": [
    {
      "source": "/",
      "has": [
        {
          "type": "host",
          "value": "app.fluxfeed.news"
        }
      ],
      "destination": "/app",
      "permanent": false
    }
  ],
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-cache, no-store, must-revalidate"
        }
      ]
    }
  ]
}
